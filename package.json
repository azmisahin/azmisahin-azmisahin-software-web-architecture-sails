{
  "name": "web-architecture-sails",
  "version": "1.0.0",
  "description": "Sails.js is a Model-View-Controller web application framework developed in the Node.js environment. It is designed to make it easy to build web applications and APIs.",
  "directories": {
    "doc": "docs"
  },
  "main": "./src",
  "scripts": {
    "format": "prettier --write . !coverage",
    "lint": "eslint . --fix",
    "test": "jest --passWithNoTests --detectOpenHandles --collectCoverage",
    "coverage": "jest --collectCoverage --passWithNoTests --detectOpenHandles",
    "build": "npm run format && npm run lint && npm run test",
    "start": "node src",
    "prod": "cross-env NODE_ENV=production npm run start",
    "dev": "cross-env NODE_ENV=development npm run start",
    "test:e2e": "nightwatch -e firefox,chrome",
    "automation": "npm run dev && npm run test:e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azmisahin/azmisahin-azmisahin-software-web-architecture-sails.git"
  },
  "keywords": [
    "sails",
    "javascript",
    "web",
    "framework",
    "nodejs",
    "express",
    "socket.io",
    "vue.js",
    "rest",
    "api",
    "orm",
    "mysql",
    "postgresql",
    "mongodb",
    "redis",
    "oracle",
    "mssql",
    "sqllite",
    "continuous-integration",
    "devops"
  ],
  "author": "azmisahin@outlook.com",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/azmisahin/azmisahin-azmisahin-software-web-architecture-sails/issues"
  },
  "homepage": "https://github.com/azmisahin/azmisahin-azmisahin-software-web-architecture-sails#readme",
  "dependencies": {
    "cross-env": "^7.0.2"
  },
  "devDependencies": {
    "chromedriver": "^83.0.0",
    "eslint": "^7.1.0",
    "geckodriver": "^1.19.1",
    "jest": "^26.0.1",
    "nightwatch": "^1.3.5",
    "prettier": "^2.0.5",
    "selenium-server": "^3.141.59"
  }
}
